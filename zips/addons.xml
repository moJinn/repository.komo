<?xml version="1.0" encoding="UTF-8"?>
<addons>
<addon id="plugin.program.mowizard" name="[B][COLOR dodgerblue]MO[/COLOR][/B] Wizard" version="1.0" provider-name="MO">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>executable</provides>
  </extension>
  <extension point="xbmc.service" library="startup.py" start="startup" />
  <extension point="xbmc.addon.metadata">
    <summary lang="en"></summary>
    <description lang="en">This Wizard offers many maintenance features, selective add-on data saving, and automated build installs.</description>
    <platform>all</platform>
  </extension>
</addon>

<addon id="repository.komo" name="KOMO Repository" version="1.0.0" provider-name="KOMO">
    <extension point="xbmc.addon.repository" name="KOMO Repository">

        <dir>
            <info compressed="false">https://raw.githubusercontent.com/moJinn/repository.komo/master/zips/addons.xml</info>
            <checksum>https://raw.githubusercontent.com/moJinn/repository.komo/master/zips/addons.xml.md5</checksum>
            <datadir zip="true">https://raw.githubusercontent.com/moJinn/repository.komo/master/zips/</datadir>
        </dir>

    </extension>
    <extension point="xbmc.addon.metadata">
        <summary>KOMO Repository</summary>
        <description>KOMO</description>
        <disclaimer>KOMO</disclaimer>
        <platform>all</platform>
        <assets>
            <icon>icon.png</icon>
            <fanart>fanart.jpg</fanart>
        </assets>
    </extension>
</addon>
</addons>
